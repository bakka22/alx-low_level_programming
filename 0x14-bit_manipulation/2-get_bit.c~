#include "main.h"
#include <stddef.h>
#include <stdio.h>
#include <limits.h>
/**
*get_bit - convert a base2 int to base 10 unsigned int
*@n : binary int
*@index : index
*Return: the converted number
*/
int get_bit(unsigne long int n, unsigned int index)
{
	if (index > 63)
		return (-1);

	return ((n >> index) & 1);
}

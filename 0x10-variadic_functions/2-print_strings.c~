#include <stdarg.h>
#include <stddef.h>
#include <stdio.h>
/**
*print_strings - sum numbers
*@separator : number of arguments
*@n : number of argument
*@... : elipsus
*Return: return the sum
*/
void print_strings(const char *separator, const unsigned int n, ...)
{
	unsigned int i;
	va_list nums;
	char *mid;

	if (n == 0)
		return;

	va_start(nums, n);
	
	if (separator == NULL)
	{
		for (i = 0; i < n; i++)
		{
			mid = va_arg(nums, char *);
			if (mid == NULL)
				printf("nil");
			else
				printf("%s", mid);
		}
		printf("\n");
	}
	else
	{
		for (i = 0; i < n; i++)
		{
			if(i != n - 1)
				printf("%s%s", va_arg(nums, char *), separator);
			else
				printf("%s", va_arg(nums, char *));
		}
		printf("\n");
	}
	va_end(nums);
}

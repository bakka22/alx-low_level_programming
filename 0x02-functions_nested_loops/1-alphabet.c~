#include <stdio.h>
#include "main.h"
/* more headers goes there */
/**
*main - the main function
* Return: 0 (success)
*/
int main(void)
{
	print_alphabet();
	return (0);
}

/**
*print_alphabet - print alphabets in lowercase
* Return: void
*/
void print_alphabet(void)
{
	int i;
	for (i = 97 ; i <= 122 ; i++)
		putchar(i);

	putchar(\n);
}
